package Firstpackage;

public class car {
	public static void main(String [] args) {
		String me = "billionere";
		if (me == "billionere") {
			System.out.println("buy mercedes and private jet");
		}if (me == "trillionere") { 
			System.out.println("buy bmw");
	}else {
			System.out.println("buy honda");
		
		}
		
 }
